{
	"id": "home_page",
	"label": "Home page",
	"repeatable": false,
	"json": {
		"Main": {
			"title": {
				"type": "StructuredText",
				"config": {
					"single": "heading1",
					"label": "Titre"
				}
			},
			"description": {
				"type": "Text",
				"config": {
					"label": "Description",
					"placeholder": "Description SEO"
				}
			}
		},
		"Header": {
			"socials": {
				"type": "Group",
				"config": {
					"fields": {
						"media": {
							"type": "Link",
							"config": {
								"select": "document",
								"customtypes": ["social_media"],
								"label": "Media"
							}
						}
					},
					"label": "Réseaux sociaux"
				}
			}
		},
		"Section projets": {
			"projects_title": {
				"type": "StructuredText",
				"config": {
					"single": "heading2,heading3,heading4,heading5,heading6",
					"label": "Titre"
				}
			},
			"projects_link_name": {
				"type": "Text",
				"config": {
					"label": "Texte du lien",
					"placeholder": "Lien vers la page projets"
				}
			},
			"projects_description": {
				"type": "StructuredText",
				"config": {
					"multi": "paragraph,preformatted,strong,em",
					"label": "Description"
				}
			},
			"projects_image": {
				"type": "Image",
				"config": {
					"constraint": {
						"width": 440,
						"height": 440
					},
					"thumbnails": [
						{
							"name": "@2x",
							"width": 880,
							"height": 880
						}
					],
					"label": "Image"
				}
			}
		},
		"Section équipe": {
			"team_title": {
				"type": "StructuredText",
				"config": {
					"single": "heading2,heading3,heading4,heading5,heading6",
					"label": "Titre"
				}
			},
			"team_link_name": {
				"type": "Text",
				"config": {
					"label": "Texte du lien",
					"placeholder": "Our team"
				}
			},
			"team_link": {
				"type": "Link",
				"config": {
					"allowTargetBlank": true,
					"label": "Lien",
					"select": null
				}
			},
			"team_description": {
				"type": "StructuredText",
				"config": {
					"multi": "paragraph,preformatted,strong,em",
					"label": "Description"
				}
			},
			"team_image": {
				"type": "Image",
				"config": {
					"constraint": {
						"width": 626,
						"height": 626
					},
					"thumbnails": [
						{
							"name": "@2x",
							"width": 1252,
							"height": 1252
						}
					],
					"label": "Image"
				}
			}
		},
		"Présentation équipe": {
			"team_members": {
				"type": "Group",
				"config": {
					"fields": {
						"image_bitmap": {
							"type": "Image",
							"config": {
								"constraint": {
									"width": 600,
									"height": 600
								},
								"thumbnails": [],
								"label": "image_bitmap"
							}
						},
						"name": {
							"type": "Text",
							"config": {
								"label": "Prénom et nom"
							}
						},
						"link": {
							"type": "Link",
							"config": {
								"allowTargetBlank": true,
								"label": "Lien",
								"placeholder": "https://linkedin.com/user",
								"select": null
							}
						},
						"display": {
							"type": "Boolean",
							"config": {
								"placeholder_false": "Non",
								"placeholder_true": "Oui",
								"default_value": false,
								"label": "Afficher sur le site"
							}
						}
					},
					"label": "Membres de l'équipe"
				}
			}
		}
	},
	"status": true,
	"format": "custom"
}
